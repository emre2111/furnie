<div class="container landing-page-home">
<p id="home-logo">furnie</p>
  <div class="homepage-image-container" style="background-image: linear-gradient(rgba(0,0,0,0.4),rgba(0,0,0,0.4)), url('http://www.10stunninghomes.com/wp-content/uploads/minimalist-apartment-ukraine.jpg')" class="homepage-image">
    <div class="landingpage-header">

      <h1 class="h1-landing">Furnish your apartment<br><strong class="half-highlight">Fast &Simple</strong></h1>
      <p class="landingpage-para">Explore our furniture available for rent <strong>in Berlin</strong></p>
      <div class="form form-inline date-form">
        <%= simple_form_for(@booking) do |f| %>
        <div class="form">
          <%= f.input :starts_at, as: :string, input_html: { id: "start_date" },
          required: false, placeholder: "Rent from", label: false %>
          <%= f.input :ends_at, as: :string, input_html: { id: "end_date" },
          required: false, placeholder: "Until ", label: false %>
          <%= f.button :submit, "Find furniture", class: "btn-first btn-landing"%>
          </div>
        <% end %>
      </div>
    </div>
</div>
</div>
